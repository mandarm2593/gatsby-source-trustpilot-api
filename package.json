{
  "name": "gatsby-source-trustpilot-api",
  "version": "1.2.6",
  "main": "index.js",
  "description": "Creates TRUSTPILOT query nodes into Gatsby's GraphQL schema",
  "scripts": {
    "build": "npx tsc --project tsconfig.json",
    "package": "npm run lint && npm run build && npx standard-version",
    "lint": "npx eslint --config .eslintrc.js --ext .js,.ts ./src",
    "lint:fix": "npx eslint --fix --config .eslintrc.js --ext .js,.ts ./src",
    "commit": "npx git-cz",
    "tar": "npm run build && npm pack"
  },
  "files": [
    "gatsby-node.js",
    "trustpilot-helpers.js",
    "index.js"
  ],
  "author": "Mandar Mathure <mandarm2593@gmail.com>",
  "contributors": [
    "Tommy May III <tommymay37@gmail.com>"
  ],
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source",
    "trustpilot api",
    "trustpilot data"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "gatsby-cli": "^2.8.3"
  },
  "devDependencies": {
    "@types/node": "^10.14.15",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "standard-version": "^6.0.1",
    "tslint": "^5.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.2"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mandarm2593/gatsby-source-trustpilot-api.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
